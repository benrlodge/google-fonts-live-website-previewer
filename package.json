{
  "name": "google-fonts-live-website-previewer",
  "version": "0.0.1",
  "description": "Google Fonts Live Website Previewer",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "expect": "^1.16.0",
    "jquery": "^2.2.2",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.6.1",
    "merge-stream": "^1.0.0",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",  
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-select": "^0.9.1",
    "react-shallow-testutils": "^1.0.0",
    "webpack-dev-server": "^1.14.1"
  },
  "scripts": {
    "start": "npm run clean && npm run build-css && npm run build-html && webpack-dev-server --content-base dist --hot --inline",
    "build": "npm run clean && npm run build-html && webpack && npm run build-css",
    "test": "karma start --single-run=false",
    "build-html": "cp src/index.html dist/index.html",
    "build-sass": "node-sass --include-path sass src/sass/GFLP.sass temp/app.css",
    "build-css": "npm run build-sass && cat node_modules/react-select/dist/react-select.min.css temp/app.css > dist/GFLP.css",
    "clean": "rm -rf dist && mkdir -p dist"
  },
  "author": "ben lincoln lodge",
  "license": "MIT"
}
